<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Sugar</servlet-name>
		<servlet-class>com.tuyano.libro.mygaeapp.SugarServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Sugar</servlet-name>
		<url-pattern>/sugar</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.tuyano.libro.mygaeapp.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>Reset</servlet-name>
		<servlet-class>com.tuyano.libro.mygaeapp.ResetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Reset</servlet-name>
		<url-pattern>/reset</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>com.tuyano.libro.mygaeapp.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
